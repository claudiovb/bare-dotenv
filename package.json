{
  "name": "bare-dotenv",
  "version": "17.2.1",
  "description": "Loads environment variables from .env file",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "require": "./lib/main.js",
      "default": "./lib/main.js"
    },
    "./config": "./config.js",
    "./config.js": "./config.js",
    "./lib/env-options": "./lib/env-options.js",
    "./lib/env-options.js": "./lib/env-options.js",
    "./lib/cli-options": "./lib/cli-options.js",
    "./lib/cli-options.js": "./lib/cli-options.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dts-check": "tsc --project tests/types/tsconfig.json",
    "lint": "standard",
    "pretest": "npm run lint && npm run dts-check",
    "test": "brittle-bare tests/test-*.js",
    "test:basic": "bare tests/test-config.js",
    "test:env": "bare tests/test-env-options.js",
    "test:coverage": "brittle-bare tests/test-*.js",
    "prerelease": "npm test",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claudiovb/bare-dotenv"
  },
  "homepage": "https://github.com/claudiovb/bare-dotenv#readme",
  "keywords": [
    "bare-dotenv",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "readmeFilename": "README.md",
  "license": "BSD-2-Clause",
  "dependencies": {
    "bare-env": "^3.0.0",
    "bare-fs": "^4.1.6",
    "bare-os": "^3.6.1",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.1",
    "bare-subprocess": "^5.1.0",
    "bare-tty": "^5.0.2",
    "bare-utils": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "brittle": "^3.6.0",
    "decache": "^4.6.2",
    "standard": "^17.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "bare": ">=1.0.0"
  }
}
